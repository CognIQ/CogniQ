{
    "display_information": {
        "name": "CogniQ"
    },
    "features": {
        "app_home": {
            "home_tab_enabled": false,
            "messages_tab_enabled": true,
            "messages_tab_read_only_enabled": false
        },
        "bot_user": {
            "display_name": "CogniQ",
            "always_online": false
        }
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "app_mentions:read",
                "channels:history",
                "chat:write",
                "groups:history",
                "im:history",
                "mpim:history"
                
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "request_url": "https://cogniq.example.com/slack/events",
            "bot_events": [
                "app_mention",
                "message.app_home",
                "message.channels",
                "message.groups",
                "message.im",
                "message.mpim"
            ]
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}